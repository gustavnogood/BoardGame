// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id   Int    @id @default(autoincrement())
  name String
  image String
  @@map("users")
}

model Game {
  id            Int    @id @default(autoincrement())
  name          String
  category      String
  numberOfPlayers Int
  time          String
  @@map("games")
}

